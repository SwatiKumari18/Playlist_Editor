@model IEnumerable<SK2247A3.ViewModels.AlbumBaseViewModel>

<h2>Albums</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.AlbumId</td>
                <td>@item.Title</td>
            </tr>
        }
    </tbody>
</table>
