@model IEnumerable<SK2247A3.ViewModels.MediaTypeBaseViewModel>

<h2>MediaTypes</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MediaTypeId</td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>
